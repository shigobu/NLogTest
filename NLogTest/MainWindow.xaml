<Window x:Class="NLogTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NLogTest"
        mc:Ignorable="d"
        Title="NLogテスト" Width="300" SizeToContent="Height">
    <Grid>
        <StackPanel>
            <Button Content="Trace" Click="Button_Click"/>
            <Button Content="Debug" Click="Button_Click"/>
            <Button Content="Info" Click="Button_Click"/>
            <Button Content="Warn" Click="Button_Click"/>
            <Button Content="Error" Click="Button_Click"/>
            <Button Content="Fatal" Click="Button_Click"/>
        </StackPanel>
    </Grid>
</Window>
